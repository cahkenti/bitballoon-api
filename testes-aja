    <script type="text/javascript">

        var ret = "";

        var iAlert = 0;

        var leftNew = -129;

        var left = leftNew - 90;

        var aSec = [7, 15, 15, 20, 30, 60, 120];

        var iSec = 0;

        var sVersion = "";

        var iVersion = 1;

        function showArrow(event)
        {
            if (event)
            {
                event.preventDefault();
            }
            $('#email').focus();
            $("#arrow").stop().show().css({left : left, opacity : 1}).animate(
                {

                    "left" : leftNew + "px"
                },
                {
                    duration      : 800,
                    specialEasing : {
                        left : "easeOutBounce"
                    }
                });

        }

        function animateImage()
        {
            var iSec = 500;

            var firstItem = $('.containerImg').find('img').first();

            var secondItem = firstItem.next();

            firstItem.fadeOut(iSec);

            secondItem.fadeIn(iSec, function ()
            {
                secondItem.removeClass('hidden');

                var dataVal = secondItem.attr('data-val');

                var aEntities = dataVal.split(' ');

                $('.username').text(aEntities[0]);

                $('.age').text(aEntities[1]);

                firstItem.addClass('hidden').appendTo($('.containerImg'));

                setTimeout(animateImage, 5000);

            });

        }

        function counterDown()
        {
            if (iSec < aSec.length)
            {
                var ms = aSec[iSec++] * 1000;
                var nr = parseInt($('span', '.hurry').text());
                nr--;
                if (nr > 0)
                {
                    setTimeout(function ()
                    {
                        $('span', '.hurry').text(nr);
                        counterDown();
                    }, ms);
                }
            }
        }

        $(document).ready(function ()
        {
            $('#email').focus();
            showArrow();
            counterDown();
            setTimeout(animateImage, 5000);
            $('#email').keyup(function ()
            {
                $('#error').text('').hide();
            });
            $('a.showArrow').click(function (event)
            {
                showArrow(event);
            });
            $('.like-chat-btn').click(function (event)
            {
                showArrow(event);
            });
            $('a.more').click(function (event)
            {
                showArrow(event);

            });
        });
	</script>
